<p style="color: green"><%= notice %></p>

<h1>Filmes</h1>

<div id="filmes" class="filmes-index-classe">
  <% @filmes.each do |filme| %>
    <%= render filme %>
    <p class="show-filme-classe">
      <%= link_to "Show this filme", filme %>
    </p>
  <% end %>
</div>

<%= link_to "New filme", new_filme_path %>
